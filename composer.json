{
  "name": "invertus/lock",
  "license": "MIT",
  "type": "library",
  "description": "The Lock Component creates and manages locks, a mechanism to provide exclusive access to a shared resource.",
  "homepage": "https://github.com/Invertus/Lock",
  "authors": [
    {
      "name": "Invertus",
      "email": "developers@invertus.eu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "symfony/lock": "v3.4.0",
    "symfony/filesystem": "v3.4.0"
  },
  "autoload": {
    "psr-4": {
      "Invertus\\Lock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Invertus\\Lock\\Tests\\": "tests/"
    }
  }
}
